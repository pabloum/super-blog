<!DOCTYPE html>
<html>
  <head>
    <title>SuperBlog</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= link_to "Sign up", new_user_registration_path, class: "btn btn-outline-danger" %>
    <%= link_to "Log in", new_user_session_path, class: "btn btn-outline-danger" %>
    <%= yield %>
  </body>
</html>
